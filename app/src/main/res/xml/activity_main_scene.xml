<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <ConstraintSet android:id="@+id/e" >
        <Constraint
            android:layout_height="400sp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="360dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintHorizontal_bias="0.0"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="match_parent"
            android:id="@+id/imageView" />
        <Constraint
            android:layout_height="206dp"
            motion:layout_editor_absoluteX="129dp"
            android:layout_width="226dp"
            android:id="@+id/lottieAnimationView3"
            motion:layout_constraintTop_toBottomOf="@+id/imageView"
            motion:layout_editor_absoluteY="525dp"
            android:layout_marginTop="-50dp" />
        <Constraint
            android:layout_height="186dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintHorizontal_bias="1.0"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="202dp"
            android:id="@+id/lottieAnimationView2"
            android:layout_marginTop="16dp"
            motion:layout_editor_absoluteX="210dp"
            motion:transitionEasing="standard"
            motion:pathMotionArc="startVertical" />
        <Constraint
            android:layout_marginEnd="120dp"
            android:layout_height="261dp"
            android:layout_marginRight="120dp"
            android:layout_marginBottom="464dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="327dp"
            android:id="@+id/lottieAnimationView"
            motion:transitionEasing="standard"
            motion:pathMotionArc="startVertical" />
    </ConstraintSet>
    <ConstraintSet android:id="@+id/ee" >
        <Constraint
            android:layout_marginEnd="44dp"
            android:layout_height="275dp"
            android:layout_marginRight="44dp"
            android:layout_marginBottom="228dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="321dp"
            android:id="@+id/lottieAnimationView"
            motion:transitionEasing="standard"
            motion:pathMotionArc="startVertical" />
        <Constraint
            android:layout_height="235dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="235dp"
            android:id="@+id/lottieAnimationView2"
            android:layout_marginTop="56dp" />
        <Constraint
            android:layout_height="400sp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="360dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintHorizontal_bias="0.0"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="match_parent"
            android:id="@+id/imageView" />
    </ConstraintSet>
    <Transition
        motion:constraintSetStart="@+id/e"
        motion:constraintSetEnd="@+id/ee"
        motion:autoTransition="animateToStart" >
        <OnSwipe />
    </Transition>
</MotionScene>